package algoritmos;

import org.junit.Assert;
import org.junit.Before;
import org.junit.Test;

public class OrdenadorTest {

    private Ordenador<Integer> ordenador;
    
    private Integer[] l1D = {1, 2};
    private Integer[] l1O = {1, 2};

    private Integer[] l2D = {1, 2};
    private Integer[] l2O = {1, 2};

    private Integer[] l3D = {1, 2};
    private Integer[] l3O = {1, 2};

    private Integer[] l4D = {1, 2};
    private Integer[] l4O = {1, 2};


    
    

    @Before
    public void setUp() throws Exception {
        this.ordenador = new Ordenador<>();
    }


    @Test
    public void testel1() {
        ordenador.ordena(l1D);
        Assert.assertArrayEquals(l1D, l1O);
    }
}
