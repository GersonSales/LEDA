package algoritmos;

public class Ordenador<E> {
    public void bubbleSort(E[] lista) {
        boolean houveTroca;
        do {
            houveTroca = false;
            for (int i = 0; i < lista.length; i ++ ) {
                
                
            }
            
        }while(houveTroca);
    }
    
    
    public void imprimeLista(E[] lista) {
        System.out.print("[");
        for (E e : lista) {
            System.out.print(e + ", ");
        }
        System.out.println("]");
    }
}